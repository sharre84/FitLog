<div class="container">



<%= simple_form_for @workout, html: { class: 'form-horizontal' } do |f| %>
<%= f.input :date, input_html: { class: 'form-control' } %>
<%= f.input :workout, input_html: { class: 'form-control' } %>
<%= f.input :length, input_html: { class: 'form-control' } %>
  <br>
  <%= f.button :submit %>
<% end %>
</div>
